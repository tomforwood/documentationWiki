export JAVA_HOME=/etc/alternatives/java_sdk_1.8.0
export PATH=$JAVA_HOME/bin:$PATH

cd $OPENSHIFT_DATA_DIR

M2_HOME="$OPENSHIFT_DATA_DIR/.m2"
export M2_HOME


echo $JAVA_HOME
echo $PATH

cd $OPENSHIFT_REPO_DIR/docuwiki

mvn -version

mvn clean package -DskipTests --settings $OPENSHIFT_DATA_DIR/.openshift/settings.xml